{
  "name": "angular2-json-schema-form",
  "description": "Angular 2 JSON Schema Form builder",
  "author": "David Schnell-Davis",
  "keywords": [ "Angular 2", "JSON Schema", "Form builder" ],
  "version": "0.1.0-alpha.1",
  "license": "MIT",
  "homepage": "",
  "bugs": "",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/dschnelldavis/angular2-json-schema-form.git"
  },
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/material": "^2.0.0-alpha.11-3",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "ajv": "^4.7.7",
    "bootstrap": "^3.3.7",
    "buffer": "^5.0.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.2",
    "ng2-bootstrap": "^1.1.12",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "spectrum-colorpicker": "^1.8.0",
    "systemjs": "0.19.38",
    "zone.js": "~0.6.23"
  },
  "devDependencies": {
    "@types/ace": "0.0.32",
    "@types/ajv": "0.0.3",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.38",
    "@types/lodash": "^4.14.42",
    "@types/node": "^6.0.51",
    "brace": "^0.8.0",
    "canonical-path": "0.0.2",
    "codelyzer": "^2.0.0-beta.1",
    "concurrently": "^3.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.2",
    "typescript": "^2.1.4"
  }
}
